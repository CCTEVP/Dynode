{
  "name": "source.dynode",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && node scripts/mergeOpenapi.js && cpx \"views/**/*\" dist/views",
    "dev": "copy .env.dev .env && npm run build && ts-node-dev app.ts",
    "start": "node dist/app.js",
    "db:clean": "node scripts/databaseClean.js",
    "db:seed": "node scripts/databaseSeed.js",
    "db:reset:dev": "copy .env.dev .env && npm run db:clean && npm run db:seed",
    "db:reset:stag": "copy .env.stag .env && npm run db:clean && npm run db:seed",
    "db:reset:prod": "copy .env.prod .env && npm run db:clean && npm run db:seed",
    "compose": "cd.. && docker-compose up source --build -d",
    "build:dev": "copy .env.dev .env && npm run build",
    "build:stag": "copy .env.stag .env && docker build -t source-dynode:latest .",
    "build:prod": "copy .env.prod .env && docker build -t source-dynode:latest .",
    "consolidate": "node ../scripts/consolidateProject.cjs .",
    "document": "node ../scripts/documentProject.cjs ."
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "~1.9.1",
    "multer": "^2.0.1",
    "pug": "2.0.0-beta11",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cpx": "^1.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "main": "scripts/mergeOpenapi.js"
}
