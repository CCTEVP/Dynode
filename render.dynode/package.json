{
  "name": "source.dynode",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:components && tsc && cpx \"views/**/*\" dist/views",
    "build:components": "tsc --project tsconfig.inplace.json",
    "build:docker": "docker build -t render-dynode:latest .",
    "start": "node dist/app.js",
    "dev": "npm run build && ts-node-dev app.ts"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "clean-css": "^5.3.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "terser": "^5.43.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/clean-css": "^4.2.11",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^24.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cpx": "^1.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
