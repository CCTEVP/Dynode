{
  "name": "build.dynode",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "copy .env.dev .env && npm run build && vite",
    "compose": "cd.. && docker-compose up builder --build -d",
    "build:dev": "copy .env.dev .env && npm run build",
    "build:stag": "copy .env.stag .env && docker build --build-arg APP_ENV=staging --build-arg PFX_PASSWORD=YourVeryStrongAndSecretPasswordHere -t build-dynode:latest .",
    "build:prod": "copy .env.prod .env && docker build --build-arg APP_ENV=production --build-arg PFX_PASSWORD=YourVeryStrongAndSecretPasswordHere -t build-dynode:latest .",
    "lint": "eslint .",
    "preview": "vite preview",
    "consolidate": "node ../scripts/consolidateProject.cjs .",
    "document": "node ../scripts/documentProject.cjs ."
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@types/dagre": "^0.7.53",
    "@xyflow/react": "^12.10.0",
    "antd": "^5.26.7",
    "dagre": "^0.8.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-grid-layout": "^1.5.2",
    "react-router-dom": "^7.6.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.0.8",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "lightningcss": "^1.31.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "overrides": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "browser": {
    "./src/services/logger.ts": "./src/services/logger.browser.ts"
  }
}
